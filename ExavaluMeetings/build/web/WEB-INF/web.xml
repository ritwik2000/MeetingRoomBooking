        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/AdminLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CancelMeeting</servlet-name>
        <url-pattern>/CancelMeeting</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayBookedMeetings</servlet-name>
        <url-pattern>/DisplayBookedMeetings</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Employeeview_event</servlet-name>
        <url-pattern>/Employeeview_event</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminCancelMeetings</servlet-name>
        <url-pattern>/AdminCancelMeetings</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Home1.jsp</welcome-file>
    </welcome-file-list>
</web-app>
